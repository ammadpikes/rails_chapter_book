<h1>Reset Your Password</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
     <%= form_tag password_reset_index_path do %>
      <%= label_tag :Email%>
      <%=  email_field_tag :email %>
      <br />
      <%= submit_tag "Reset Password", class: "btn btn-primary"  %>
    <% end %>
  </div>
</div>
